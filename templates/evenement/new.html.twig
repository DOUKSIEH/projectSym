{% extends 'base.html.twig' %}

{% block title %}New Evenement{% endblock %}

{% block body %}
    <div class="container mt-3">
      
        <div class="row">
            <div class="col-10">
                <h1>Créer un nouveau evenement</h1>

                    {{ include('evenement/_form.html.twig') }}
               </div>
            <div class="col-2 mt-5">
                <button class="btn btn-success mt-5 mb-3">{{ button_label|default('Enregistrer') }}</button>
                <a href="{{ path('evenement_index') }}" class="btn btn-primary">Retour à la liste</a>
                
                {{ form_end(form) }} 
               
            </div>
        </div>

    </div>
{% endblock %}
